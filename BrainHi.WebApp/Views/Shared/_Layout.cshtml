
<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Libs CSS -->
    <link rel="stylesheet" href="~/assets/fonts/Feather/feather.css">
    <link rel="stylesheet" href="~/assets/libs/@@fancyapps/fancybox/dist/jquery.fancybox.min.css">
    <link rel="stylesheet" href="~/assets/libs/aos/dist/aos.css">
    <link rel="stylesheet" href="~/assets/libs/choices.js/public/assets/styles/choices.min.css">
    <link rel="stylesheet" href="~/assets/libs/flickity-fade/flickity-fade.css">
    <link rel="stylesheet" href="~/assets/libs/flickity/dist/flickity.min.css">
    <link rel="stylesheet" href="~/assets/libs/highlightjs/styles/vs2015.css">
    <link rel="stylesheet" href="~/assets/libs/jarallax/dist/jarallax.css">
    <link rel="stylesheet" href="~/assets/libs/quill/dist/quill.core.css" />

    <!-- Map -->
    <link href='https://api.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.css' rel='stylesheet' />

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/assets/css/theme.min.css">

    <title>BrainHi</title>

</head>
<body class="bg-light">
    <!-- NAVBAR
    ================================================== -->
    <nav class="navbar navbar-expand-lg navbar-light bg-white border-bottom">
        <div class="container">

            <!-- Brand -->
            <a class="navbar-brand" href="/">
                BrainHi
            </a>

        </div>
    </nav>

    @if (TempData["Notification"] != null)
    {
        <!-- NOTIFICATION -->
        <div class="alert alert-@TempData["NotificationType"] rounded-0 mb-0">
            <div class="container-md">
                <div class="row">
                    <div class="col-12">

                        <!-- Text -->
                        <p class="text-center mb-0">
                            @TempData["Notification"]
                        </p>

                    </div>
                </div>
            </div>
        </div>
    }
    <!-- CONTENT -->
    @RenderBody()

    <!-- SHAPE
    ================================================== -->
    <div class="position-relative">
        <div class="shape shape-bottom shape-fluid-x svg-shim text-dark">
            <svg viewBox="0 0 2880 48" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M0 48h2880V0h-720C1442.5 52 720 0 720 0H0v48z" fill="currentColor" /></svg>
        </div>
    </div>

    <!-- FOOTER
    ================================================== -->
    <footer class="py-8 py-md-11 bg-dark">
        <div class="container">
            <div class="row">
                <div class="col-12 col-md-4 col-lg-3">

                    <!-- Brand -->
                    <h1 class="footer-brand mb-2">BrainHi</h1>

                    <!-- Text -->
                    <p class="text-gray-700 mb-2">
                        A better way to connect to your doctor.
                    </p>

                    <!-- Social -->
                    <ul class="list-unstyled list-inline list-social mb-6 mb-md-0">
                        <li class="list-inline-item list-social-item mr-3">
                            <a href="https://github.com/jossean-rivera/BrainHi" target="_blank" class="text-decoration-none h1">
                                <i class="fab fa-github text-gray-600"></i>
                            </a>
                        </li>
                    </ul>

                </div>
                <div class="col-6 col-md-4 offset-md-4 col-lg-2 offset-lg-0 ml-md-auto">

                    <!-- Heading -->
                    <h6 class="font-weight-bold text-uppercase text-gray-700">
                        Resources
                    </h6>

                    <!-- List -->
                    <ul class="list-unstyled text-muted mb-0">
                        <li class="mb-3">
                            <a href="https://github.com/jossean-rivera/BrainHi" class="text-reset">
                                Source Code
                            </a>
                        </li>
                        <li class="mb-3">
                            <a href="https://github.com/jossean-rivera/" class="text-reset">
                                Author
                            </a>
                        </li>
                        <li class="mb-3">
                            <a href="https://asp.net" class="text-reset">
                                ASP.NET
                            </a>
                        </li>
                        <li class="mb-3">
                            <a href="https://dot.net" class="text-reset">
                                .NET Core
                            </a>
                        </li>
                        <li class="mb-3">
                            <a href="https://fontawesome.com" class="text-reset">
                                Fontawesome
                            </a>
                        </li>
                        <li class="mb-3">
                            <a href="https://getbootstrap.com" class="text-reset">
                                Bootstrap
                            </a>
                        </li>
                        <li>
                            <a href="https://themes.getbootstrap.com/product/landkit/" class="text-reset">
                                Theme
                            </a>
                        </li>
                    </ul>

                </div>
            </div> <!-- / .row -->
        </div> <!-- / .container -->
    </footer>

    <!-- JAVASCRIPT
    ================================================== -->
    <!-- Libs JS -->
    <script src="~/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/libs/@@fancyapps/fancybox/dist/jquery.fancybox.min.js"></script>
    <script src="~/assets/libs/aos/dist/aos.js"></script>
    <script src="~/assets/libs/choices.js/public/assets/scripts/choices.min.js"></script>
    <script src="~/assets/libs/countup.js/dist/countUp.min.js"></script>
    <script src="~/assets/libs/dropzone/dist/min/dropzone.min.js"></script>
    <script src="~/assets/libs/flickity/dist/flickity.pkgd.min.js"></script>
    <script src="~/assets/libs/flickity-fade/flickity-fade.js"></script>
    <script src="~/assets/libs/highlightjs/highlight.pack.min.js"></script>
    <script src="~/assets/libs/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="~/assets/libs/isotope-layout/dist/isotope.pkgd.min.js"></script>
    <script src="~/assets/libs/jarallax/dist/jarallax.min.js"></script>
    <script src="~/assets/libs/jarallax/dist/jarallax-video.min.js"></script>
    <script src="~/assets/libs/jarallax/dist/jarallax-element.min.js"></script>
    <script src="~/assets/libs/quill/dist/quill.min.js"></script>
    <script src="~/assets/libs/smooth-scroll/dist/smooth-scroll.min.js"></script>
    <script src="~/assets/libs/typed.js/lib/typed.min.js"></script>

    <!-- Map -->
    <script src='https://api.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.js'></script>

    <!-- Theme JS -->
    <script src="~/assets/js/theme.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
